---
const { name } = Astro.props;

import IconGithub from "./icons/IconGithub.astro";
import IconLinkedin from "./icons/IconLinkedin.astro";
import IconMoon from "./icons/IconMoon.astro";
import IconSun from "./icons/IconSun.astro";

export const iconList = {
  Github: IconGithub,
  Linkedin: IconLinkedin,
  Moon: IconMoon,
  Sun: IconSun,
};

const Component = iconList[name];
---

<Component />
